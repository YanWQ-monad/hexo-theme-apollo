ul.nav.nav-list
    each value, key in theme.menu
        li.nav-list-item
            - var re = /^(http|https):\/\/*/gi;
            - var tar = re.test(value) ? "_blank" : "_self"
            - var act = !re.test(value) && "/"+page.current_url === value
            - var rel = tar === "_blank" ? "noopener" : false
            a.nav-list-link(class={active: act} href=url_for(value) target=tar, rel=rel)
                != key.toUpperCase()
